{
  "name": "tw-syncer",
  "version": "1.0.0",
  "description": "Tw syncer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest -c jest.config.unit.js",
    "test:unit": "npm run clean && npm run build & jest -c jest.config.unit.js",
    "test:integration": "npm run clean && npm run build & jest -c jest.config.integration.js",
    "lint": "node_modules/tslint/bin/tslint -p tsconfig.json -c tslint.json 'src/**/*.{ts,tsx}' -o lint-result.json -t json",
    "build": "npm run clean:build && tsc",
    "build:lambda": "npm run clean:lambda && mkdir lambda && cd lambda && cp ../package.json . && npm i --production && rm *.json && zip -r lambda.zip . && rm -rf node_modules",
    "clean:build": "rm -rf dist",
    "clean:lambda": "rm -rf lambda",
    "clean:doc": "rm -rf doc",
    "clean:test": "rm -rf coverage",
    "clean:all": "npm run clean:build && npm run clean:doc && npm run clean:test && npm run clean:lambda",
    "doc": "./node_modules/.bin/typedoc --out doc --mode modules src/",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jianhan/tw-syncer.git"
  },
  "keywords": [
    "tw",
    "typescript"
  ],
  "author": "Jian Han <jianhan2018@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jianhan/tw-syncer/issues"
  },
  "homepage": "https://github.com/jianhan/tw-syncer#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/node": "^13.9.8",
    "aws-lambda": "^1.0.5",
    "aws-sdk": "^2.650.0",
    "class-validator": "^0.11.1",
    "dotenv": "^8.2.0",
    "http-status-codes": "^1.4.0",
    "immutable": "^4.0.0-rc.12",
    "jianhan-fp-lib": "^1.2.26",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "sanctuary": "^3.0.0",
    "sanctuary-def": "^0.21.1",
    "sanctuary-identity": "^2.1.0",
    "sanctuary-maybe": "^2.1.0",
    "sprintf": "^0.1.5",
    "twitter": "^1.7.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.47",
    "@types/faker": "^4.1.11",
    "@types/http-status-codes": "^1.2.0",
    "@types/immutable": "^3.8.7",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/sanctuary": "^0.14.2",
    "@types/twitter": "^1.7.0",
    "faker": "^4.1.0",
    "jest": "^25.2.4",
    "jsverify": "^0.8.4",
    "ts-jest": "^25.3.0",
    "tslint": "^6.1.0",
    "tslint-clean-code": "^0.2.10",
    "tslint-sonarts": "^1.9.0",
    "typedoc": "^0.17.3",
    "typescript": "^3.8.3"
  }
}
